{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"semicolon": false,
		"indent": [true, "tabs", 4],
		"whitespace": false,
		"arrow-parens": false,
		"trailing-comma": false,
		"prefer-const": true,
		"no-var-keyword": true,
		"eofline": true
	},
	"rulesDirectory": []
}
