section
	header
		h2 Editor Panel - Edits
	
	include ../header.jade
	
	h3 Log
	if edits
		table.edits
			tbody
				each edit in edits
					tr
						td
							a.ajax(href='/anime/' + edit.id)= edit.title
						td= edit.providerName
						td= edit.edited
						td
							a.ajax(href='/+' + users[edit.editedBy].nick)= users[edit.editedBy].nick