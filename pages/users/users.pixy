component Users(users []*arn.User)
	h1.page-title Users

	UsersTabs

	.user-avatars
		each user in users
			.mountable
				Avatar(user)

component UsersTabs
	.tabs
		Tab("Active", "users", "/users")
		Tab("Watching", "tv", "/users/anime/watching")
		Tab("Osu", "gamepad", "/users/osu")
		Tab("Staff", "user-secret", "/users/staff")