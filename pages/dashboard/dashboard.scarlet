.schedule-item-link,
.schedule-item-title
	white-space nowrap
	text-overflow ellipsis
	overflow hidden

.schedule-item-link
	horizontal
	align-items center

.schedule-item-date
	text-align right

.footer-element
	:after
		content " | "
		color text-color
		opacity 0.5
	
	:last-child
		:after
			display none